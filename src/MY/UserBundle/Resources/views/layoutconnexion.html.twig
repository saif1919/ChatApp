                        &copy; 2017 <a target="_blank" href="https://www.facebook.com" title="">FACEBOOK</a>. All Rights Reserved.
                    </div>
                    <div class="col-sm-6">

                    </div>
                </div>
            </div>
        </footer><!--/#footer-->
    {% endblock %}
    {% block script %}

        <script src="{{ asset('bundles/myuser/js/jquery.js') }}"></script>
        <script src="{{ asset('bundles/myuser/js/bootstrap.min.js') }}"></script>
        <script src="{{ asset('bundles/myuser/js/jquery.prettyPhoto.js') }}"></script>
        <script src="{{ asset('bundles/myuser/js/jquery.isotope.min.js') }}"></script>
        <script src="{{ asset('bundles/myuser/js/main.js') }}"></script>
        <script src="{{ asset('bundles/myuser/js/wow.min.js') }}"></script>
