        <link href="{{ asset('bundles/myuser/css/animate.min.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/myuser/css/prettyPhoto.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/myuser/css/main.css') }}" rel="stylesheet">
        <link href="{{ asset('bundles/myuser/css/responsive.css') }}" rel="stylesheet">

       {%  if app.request.get('_route') =="fos_user_registration_register" %}
